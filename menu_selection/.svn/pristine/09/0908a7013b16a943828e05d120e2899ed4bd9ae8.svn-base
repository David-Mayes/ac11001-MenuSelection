public class Gymnast extends Competitor
{
    private String country;
    private double score;
    
    public Gymnast(){
        super();
        country = "N/A";
        score = 0.0;
    }
    
    public String getCountry(){
        return country;
    }
    
    public double getScore(){
        return score;
    }
    
    public void setValues(String country, double score){
        this.country = country;
        this.score = score;
    }
}
